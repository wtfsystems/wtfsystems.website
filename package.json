{
  "name": "jekyll-vue-template",
  "version": "2.0.0",
  "description": "A starter template for Jekyll projects with Vue.js and Vue Single File Components, complete with webpack.",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "jekyll:watch": "bundle exec jekyll build --drafts --watch --incremental",
    "jekyll:build": "bundle exec jekyll build --drafts",
    "serve": "browser-sync start --config browser-sync.js"
  },
  "keywords": [
    "jekyll",
    "vue",
    "ssg"
  ],
  "author": "Christopher Murphy",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "prismjs": "^1.23.0",
    "showdown": "^1.9.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "browser-sync": "^2.26.14",
    "clean-webpack-plugin": "*",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "html-webpack-plugin": "^5.3.1",
    "markdown-to-json": "^0.5.4",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "vue-async-computed": "^3.9.0",
    "vue-loader": "^15.9.7",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0"
  }
}
